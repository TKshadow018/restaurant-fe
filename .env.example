# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id_here
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id_here
REACT_APP_FIREBASE_APP_ID=your_firebase_app_id_here

# Google Maps API Configuration (for address validation and map picker)
# Get your API key from: https://console.developers.google.com/
# Enable the following APIs: Maps JavaScript API, Geocoding API, Places API
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Application Configuration
REACT_APP_RESTAURANT_NAME="Your Restaurant Name"
REACT_APP_RESTAURANT_EMAIL=contact@yourrestaurant.com
REACT_APP_RESTAURANT_PHONE="+46 XXX XXX XX XX"
REACT_APP_RESTAURANT_ADDRESS="Your Restaurant Address, City, Sweden"

# Development/Production Environment
NODE_ENV=development
GENERATE_SOURCEMAP=false
